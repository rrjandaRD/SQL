INSERT INTO Meals (CompanyID, MealName, Size_grams, Calories, Cal_per100g, Protein, Pro_per100g, CHO, CHO_per100g, Fiber, Fiber_per100g, Organic, QualityPro, HighFiber, GF, Prepped) 
