CREATE TABLE Meals (
    ID            INTEGER PRIMARY KEY AUTOINCREMENT,
    CompanyID     INTEGER REFERENCES Companies (CompanyID),
    MealName      TEXT,
    Size_grams    NUMERIC,
    Calories      NUMERIC,
    Cal_per100g   NUMERIC,
    Protein       NUMERIC,
    Pro_per100g   NUMERIC,
    CHO           NUMERIC,
    CHO_per100g   NUMERIC,
    Fiber         NUMERIC,
    Fiber_per100g NUMERIC,
    Organic       NUMERIC,
    QualityPro    NUMERIC,
    HighFiber     NUMERIC,
    GF            NUMERIC,
    Prepped       NUMERIC
);

